


public class SuperAthlete extends Athlete  {
	
	
	
	public SuperAthlete(String _ID, String _name, int _age, String _state) {
		super(_ID, _name, _age, _state);
	}

	
	public int compete() {
		// TODO Auto-generated method stub
		
		String currentGame = "";
		if (currentGame.equals("running")) {
			return 10 + rand.nextInt(11);
		}
		
		else if (currentGame.equals("cycling")) {
			return 500 + rand.nextInt(301);
		}
		
		
		else
		
			return 100 + rand.nextInt(101);
		
	}

}
